{
  "name": "react-native-ts-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --skipflow",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "prebuild": "node node_modules/.bin/rimraf build",
    "build:ts": "tsc",
    "build": "tsc && cp -r ./images ./build/images",
    "dev": "cp -r ./images ./build/images && tsc --watch",
    "pretest": "npm run build",
    "test": "jest",
    "postinstall": "node_modules/.bin/rn-nodeify --install crypto --hack && npm run build:mqttClient",
    "build:mqttClient": "webpack node_modules/mqtt/mqtt.js node_modules/mqtt/mqttClient.js --output-library mqtt --output-library-target commonjs2"
  },
  "dependencies": {
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.70",
    "@types/qs": "^6.5.0",
    "@types/react-redux": "^4.4.46",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^1.2.6",
    "@types/redux-thunk": "^2.1.0",
    "jwt-decode": "^2.2.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "mqtt": "^2.15.0",
    "qs": "^6.5.0",
    "react": "^16.2.0",
    "react-native": "^0.51.0",
    "react-native-crypto": "^2.0.2",
    "react-native-randombytes": "^2.2.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "react_native_mqtt": "^1.1.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/react": "^16.0.34",
    "@types/react-native": "^0.51.9",
    "@types/react-navigation": "^1.0.12",
    "@types/react-test-renderer": "^16.0.0",
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "jest": "19.0.2",
    "react-test-renderer": "16.0.0-alpha.6",
    "rimraf": "^2.6.1",
    "rn-nodeify": "^7.0.1",
    "ts-jest": "^19.0.14",
    "typescript": "^2.6.2"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "react-native": {
    "crypto": "react-native-crypto"
  },
  "browser": {
    "crypto": "react-native-crypto"
  }
}
